#include <iostream>
using namespace std;

int main() {
    
    int speedLimit = 65;
    int currentSpeed;
    
    cout << "Enter speed: ";
    cin >> currentSpeed;
    
    if (currentSpeed > speedLimit)
    {
        cout << "Too fast" << endl;
    }
    else if ((currentSpeed > 45) && (currentSpeed <= speedLimit))
    {
        cout << "Thank you for driving the speed limit." << endl;
    }
    else if (currentSpeed < 45)
    {
        cout << "Too slow." << endl;
    }
    
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int i;
    int j;
    
    for (i = 1; i <= 5; i++) {
        for (j = 1; j <= 5; j++) {
            cout << "*";
        }
        cout << endl;
    }
    
    cout << endl;
    
    for (i = 0; i < 5; i++) {
        for (j = 1; j <= i + 1; j++) {
            cout << "*";
        }
        cout << endl;
    }
    
    cout << endl;
    
    for (i = 5; i >= 1; i--) {
        // cout << "*"; // this prints out *****
        for (j = 1; j <= i; j++) {
            cout << "*";
        }
        
        cout << endl;
    }
    
    
    return 0;
}

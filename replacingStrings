//Given string stringValue on one line, string replaceStr on a second line, and integer deleteLen on a third line, replace the first deleteLen characters in stringValue with the contents of replaceStr.
//
//Ex: If the input is:
//
//Fuzzy bear
//Happ
//4
//
//then the output is:
//
//Happy bear

#include <iostream>
#include <string>
using namespace std;

int main() {
   string stringValue;
   string replaceStr;
   int deleteLen;

   getline(cin, stringValue);
   getline(cin, replaceStr);
   cin >> deleteLen;

    stringValue.replace(0, 4, replaceStr);

   cout << stringValue << endl;
   
   return 0;
}

#include <iostream>
#include <vector>
using namespace std;

int main() {
    int grade[2][3]; //row=0-1 column=0-2
    int i; // associated with row
    int j; // associated with column
    int sum = 0;
    int totalSum;
    
    // i < 2 cuz it is the row
    for (i = 0; i < 2; i++) { // row
        for (j = 0; j < 3; j++) { // column
            cout << "input grade: ";
            cin >> grade[i][j];
            sum = sum + grade[i][j];
//            cout << sum << endl;
        }
        
    }
    
    totalSum = sum;
    
    cout << "Sum is equal to " << totalSum << endl;
    
    
    return 0;
}

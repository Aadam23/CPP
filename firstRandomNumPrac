#include <iostream>
#include <cstdlib>
using namespace std;

int main() {
    int smallestInput;
    int greatestInput;
    
    cout << "Enter your range: ";
    cin >> smallestInput;
    cin >> greatestInput;
    
    cout << (rand() % greatestInput - smallestInput + 1) + smallestInput << endl;
    cout << (rand() % greatestInput - smallestInput + 1) + smallestInput << endl;
    cout << (rand() % greatestInput - smallestInput + 1) + smallestInput << endl;
    
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    
    int one;
    int two;
    int three;
    
    cin >> one >> two >> three;
    
    if ((one <=  two) && (one <= three)) {
        cout << one << endl;
    }
    else if ((two <= one) && (two <= three)) {
        cout << two << endl;
    }
    else if ((three <= one) && (three <= two)) {
        cout << three << endl;
    }
}
